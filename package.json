    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.5.0",
    "dotenv": "^8.2.0",
    "email-validator": "^2.0.4",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
